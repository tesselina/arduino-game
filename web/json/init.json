{
    "canvas": {
        "element": "canvas",
        "width": 500,
        "height": 550,
        "topbar": 50,
        "color": "#413746"
    },
    "game": {
        "fps": 40,
        "timeoutRing": {
            "@some": "int",
            "min": 200,
            "max": 500
        },
        "welcome": "Sammle Ringe",
        "gameover": "Du hast verloren",
        "start": "Starten",
        "stop": "Beenden"
    },
    "model": {
        "game_button": {},
        "border": {
            "vr": 10,
            "ar": 0.5,
            "r": {
                "start": 250,
                "range": {
                    "@some": "int",
                    "min": 50,
                    "max": 250
                }
            }
        },
        "player": {
            "r": {
                "start": 50,
                "min": 25,
                "max": 250
            }
        },
        "ring": {
            "limit": 1,
            "r": {
                "@some": "int",
                "max": 240
            }
        },
        "info": {
            "value": ""
        },
        "score": {
            "template": "Ringe: $1",
            "value": 0
        }
    },
    "view": {
        "ampel": {
            "element": "ampel",
            "green": "#88ff76",
            "red": "#ff7a7a",
            "size": 30
        },
        "game_button": {
            "elementID": "game_button"
        },
        "player": {
            "color": "#2c2c32"
        },
        "border": {
            "color": "#eceaff"
        },
        "ring": {
            "color": "rgba(225,225,225,0)",
            "borderWidth": 6,
            "borderColor": [
                "#88ff76",
                "#76cc77",
                "#ff7a7a",
                "#ffda7a",
                "#9d7aff"
            ]
        },
        "info": {
            "element": "info"
        },
        "score": {
            "element": "score"
        }
    }
}