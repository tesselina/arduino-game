{
    "canvas": {
        "element": "canvas",
        "width": 500,
        "height": 550,
        "topbar": 50,
        "color": "#413746"
    },
    "game": {
        "fps": 60,
        "timeoutRing": {
            "@some": "int",
            "min": 200,
            "max": 500
        },
        "welcome": "Sammle Ringe",
        "gameover": "Du hast verloren",
        "start": "Starten",
        "stop": "Beenden"
    },
    "model": {
        "game_button": {},
        "border": {
            "vr": 5,
            "r": {
                "start": 250,
                "range": {
                    "@some": "int",
                    "min": 50,
                    "max": 250
                }
            }
        },
        "player": {
            "r": {
                "start": 50,
                "min": 25,
                "max": 250
            }
        },
        "ring": {
            "limit": 1,
            "r": {
                "@some": "int",
                "max": 240
            }
        },
        "info": {
            "pos": {
                "x": 10,
                "y": 130
            }
        },
        "score": {
            "pos": {
                "x": 5,
                "y": 3
            },
            "template": "Punkte: $1",
            "value": 0
        }
    },
    "view": {
        "game_button": {
            "elementID": "game_button"
        },
        "player": {
            "color": "#413746"
        },
        "border": {
            "color": "#CCC"
        },
        "ring": {
            "color": "#CCC",
            "borderWidth": 5,
            "borderColor": [
                "#88ff76",
                "#76cc77",
                "#ff7a7a",
                "#ffda7a",
                "#9d7aff"
            ]
        },
        "info": {
            "color": "",
            "font": "bold 25px Verdana, Geneva, sans-serif",
            "textAlign": "center"
        },
        "score": {
            "color": "#000000",
            "font": "bold 20px \"Courier New\", Courier, monospace",
            "textBaseline": "top"
        }
    }
}